<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="parameterViewModel"
            type="com.example.boardinghousemanage.screens.parameter.ParameterViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView7"
            style="@style/ValueText20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/khong_value"
            android:textSize="19sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/rac_edit_par"
            app:layout_constraintStart_toEndOf="@+id/rac_edit_par" />

        <TextView
            android:id="@+id/textView6"
            style="@style/ValueText20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/khong_value"
            android:textSize="19sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/net_edit_par"
            app:layout_constraintStart_toEndOf="@+id/net_edit_par" />

        <TextView
            android:id="@+id/textView5"
            style="@style/ValueText20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/khong_value"
            android:textSize="19sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/xe_edit_par"
            app:layout_constraintStart_toEndOf="@+id/xe_edit_par" />

        <TextView
            android:id="@+id/textView4"
            style="@style/ValueText20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/khong_value"
            android:textSize="19sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/phong_edit_par"
            app:layout_constraintStart_toEndOf="@+id/phong_edit_par" />

        <TextView
            android:id="@+id/textView3"
            style="@style/ValueText20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/khong_value"
            android:textSize="19sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/nuoc_edit_par"
            app:layout_constraintStart_toEndOf="@+id/nuoc_edit_par" />

        <TextView
            android:id="@+id/textView2"
            style="@style/ValueText20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/khong_value"
            android:textSize="19sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/dien_edit_par"
            app:layout_constraintStart_toEndOf="@+id/dien_edit_par" />

        <TextView
            android:id="@+id/dg_dien_par"
            style="@style/ValueText20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="32dp"
            android:text="@string/don_gia_dien"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/dg_nuoc_par"
            style="@style/ValueText20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:text="@string/don_gia_nuoc"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dg_dien_par" />

        <TextView
            android:id="@+id/dg_phong_par"
            style="@style/ValueText20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:text="@string/tien_phong"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dg_nuoc_par" />

        <TextView
            android:id="@+id/dg_xe_par"
            style="@style/ValueText20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:text="@string/dg_xe"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dg_phong_par" />

        <TextView
            android:id="@+id/dg_net_par"
            style="@style/ValueText20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:text="@string/tien_net"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dg_xe_par" />

        <TextView
            android:id="@+id/dg_rac_par"
            style="@style/ValueText20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:text="@string/tien_rac"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dg_net_par" />

        <EditText
            android:id="@+id/dien_edit_par"
            style="@style/EditTextStyle"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:ems="10"
            android:hint="@string/hint"
            android:importantForAutofill="no"
            app:layout_constraintBaseline_toBaselineOf="@+id/dg_dien_par"
            app:layout_constraintStart_toEndOf="@+id/dg_nuoc_par"
            android:textSize="19sp" />

        <EditText
            android:id="@+id/nuoc_edit_par"
            style="@style/EditTextStyle"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:ems="10"
            android:hint="@string/hint"
            android:importantForAutofill="no"
            android:textSize="19sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/dg_nuoc_par"
            app:layout_constraintStart_toEndOf="@+id/dg_nuoc_par" />

        <EditText
            android:id="@+id/phong_edit_par"
            style="@style/EditTextStyle"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:ems="10"
            android:hint="@string/hint"
            android:importantForAutofill="no"
            android:textSize="19sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/dg_phong_par"
            app:layout_constraintStart_toEndOf="@+id/dg_nuoc_par" />

        <EditText
            android:id="@+id/xe_edit_par"
            style="@style/EditTextStyle"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:ems="10"
            android:hint="@string/hint"
            android:importantForAutofill="no"
            android:textSize="19sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/dg_xe_par"
            app:layout_constraintStart_toEndOf="@+id/dg_nuoc_par" />

        <EditText
            android:id="@+id/net_edit_par"
            style="@style/EditTextStyle"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:ems="10"
            android:hint="@string/hint"
            android:importantForAutofill="no"
            android:textSize="19sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/dg_net_par"
            app:layout_constraintStart_toEndOf="@+id/dg_nuoc_par" />

        <EditText
            android:id="@+id/rac_edit_par"
            style="@style/EditTextStyle"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:ems="10"
            android:hint="@string/hint"
            android:importantForAutofill="no"
            android:textSize="19sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/dg_rac_par"
            app:layout_constraintStart_toEndOf="@+id/dg_nuoc_par" />

        <Button
            android:id="@+id/button_xacnhan_par"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="24dp"
            android:onClick="@{() -> parameterViewModel.onXacNhan()}"
            android:text="@string/button_xacnhan"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/button_huy_par"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/button_huy_par"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:onClick="@{() -> parameterViewModel.onHuy()}"
            android:text="@string/button_huy"
            app:layout_constraintBaseline_toBaselineOf="@+id/button_xacnhan_par"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/button_xacnhan_par" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>